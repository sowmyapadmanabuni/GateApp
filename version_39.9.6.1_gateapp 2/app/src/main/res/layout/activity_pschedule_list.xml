<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".pertroling.PScheduleListActivity">

    <include android:id="@+id/toolbar_layout" layout="@layout/header_with_back" />
    <include android:id="@+id/title_block" layout="@layout/title_bar" />
    <androidx.recyclerview.widget.RecyclerView
            android:layout_marginTop="8dp"
            android:id="@+id/rcv_pat_schedules"
